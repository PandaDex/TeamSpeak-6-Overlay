{
  "name": "teamspeak-overlay",
  "productName": "teamspeak-overlay",
  "version": "1.0.1",
  "description": "Simple TeamSpeak 6 Overlay",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "vite --config vite.renderer.config.mjs src/frontend",
    "make": "electron-forge make",
    "make:win": "electron-forge make --platform win32 --targets @electron-forge/maker-squirrel",
    "make:deb": "electron-forge make --platform linux --targets @electron-forge/maker-deb",
    "publish": "electron-forge publish",
    "lint": "biome lint"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@electron-forge/cli": "^7.6.1",
    "@electron-forge/maker-deb": "^7.6.1",
    "@electron-forge/maker-rpm": "^7.6.1",
    "@electron-forge/maker-squirrel": "^7.6.1",
    "@electron-forge/maker-zip": "^7.6.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.1",
    "@electron-forge/plugin-fuses": "^7.6.1",
    "@electron-forge/plugin-vite": "^7.6.1",
    "@electron-forge/publisher-github": "^7.6.1",
    "@electron/fuses": "^1.8.0",
    "autoprefixer": "^10.4.20",
    "electron": "34.0.2",
    "postcss": "^8.5.1",
    "shadcn": "^2.3.0",
    "vite": "^5.0.12"
  },
  "keywords": [],
  "author": {
    "name": "PandaDex",
    "email": "dex@kypebot.ovh"
  },
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.4",
    "@radix-ui/react-toast": "^1.2.5",
    "@tailwindcss/vite": "^4.0.1",
    "auto-launch": "^5.0.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-squirrel-startup": "^1.0.1",
    "lucide-react": "^0.474.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.5",
    "sonner": "^1.7.3",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^4.0.1",
    "tailwindcss-animate": "^1.0.7"
  }
}